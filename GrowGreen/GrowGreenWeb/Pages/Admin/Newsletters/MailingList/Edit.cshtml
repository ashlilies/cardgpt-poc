@page
@model GrowGreenWeb.Pages.Admin.Newsletters.MailingList.EditModel

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>Email Record</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Email.Id" />
            <div class="form-group">
                <label class="control-label">Email</label>
                <input asp-for="Email.Email1" class="form-control" />
                <span asp-validation-for="Email.Email1" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="./Index">Back to List</a>
</div>

