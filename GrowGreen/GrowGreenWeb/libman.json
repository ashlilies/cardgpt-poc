{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/signalr@6.0.6",
      "destination": "wwwroot/microsoft/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "summernote@0.8.20",
      "destination": "wwwroot/lib/summernote/"
    },
    {
      "provider": "cdnjs",
      "library": "bootstrap-select@1.13.18",
      "destination": "wwwroot/lib/bootstrap-select/"
    },
    {
      "provider": "cdnjs",
      "library": "popper.js@2.11.6",
      "destination": "wwwroot/lib/popper.js/"
    }
  ]
}